{
  "name": "__MSG_supost_name__",
  "version": "0.0.1",
  "manifest_version": 2,
	"options_page": "options.html",
	"browser_action" : {
		"default_title" : "SUPost.com checker",
		"default_popup"	: "popup.html"
	},
  "background": {
    "persistent": false,
    "page": "background.html"
  },
  "default_locale": "en",
  "description": "__MSG_supost_description__",
	"icons": {
		"16": "images/favicon.png",
		"48": "images/supost48.png",
		"128": "images/supost.png"
	},
  "permissions": [
    "alarms",
    "tabs",
    "*://*.supost.com/"
   ]
}
