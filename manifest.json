{
  "name": "__MSG_supost_name__",
  "version": "0.0.1",
  "manifest_version": 2,
	"browser_action" : {
		"default_title" : "SUPost.com checker",
		"default_icon"	: "favicon.png",
		"default_popup"	: "popup.html"
	},
  "background": {
    "persistent": false,
    "page": "background.html"
  },
  "default_locale": "en",
  "description": "__MSG_supost_description__",
	"icons": {
		"16": "favicon.png",
		"48": "supost48.png",
		"128": "supost.png"
	},
  "permissions": [
    "alarms",
    "tabs",
    "webNavigation",
    "*://*.supost.com/"
   ]
}
